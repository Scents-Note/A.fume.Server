{
    "name": "a-fume-server",
    "version": "0.0.4",
    "description": "향수 정보 서비스 A.fume Server Api 문서",
    "main": "index.js",
    "type": "commonjs",
    "scripts": {
        "_prestart": "npm install",
        "start": "cross-env NODE_ENV=development npm run build && node ./dist/bin/www ",
        "test": "cross-env NODE_ENV=test mocha --recursive --exit -t 5000 ./tests/test_unit/**/*.spec.ts ./tests/test_unit/**/*.spec.js -r ts-node/register -r tsconfig-paths/register",
        "test-unit": "cross-env NODE_ENV=test mocha --recursive --exit -t 5000 ./tests/test_unit/**/*.spec.ts ./tests/test_unit/**/*.spec.js -r ts-node/register -r tsconfig-paths/register ",
        "test-integral": "cross-env NODE_ENV=production mocha --recursive --exit -t 5000 ./tests/test_integral/**/*.spec.ts -r ts-node/register -r tsconfig-paths/register --parallel",
        "test-each": "cross-env NODE_ENV=test mocha --recursive --exit -t 5000 -r ts-node/register -r tsconfig-paths/register",
        "type-check": "tsc --noEmit",
        "type-check:watch": "npm run type-check --watch",
        "build": "npm run build:js",
        "build:js": "babel src --out-dir dist --extensions \".ts,.tsx,.js\" --source-maps inline"
    },
    "keywords": [
        "swagger"
    ],
    "license": "GNU",
    "private": true,
    "dependencies": {
        "@types/swagger-ui-express": "^4.1.3",
        "@types/winston": "^2.4.4",
        "aws-sdk": "^2.1101.0",
        "chai": "^4.3.6",
        "connect": "^3.7.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "discord-webhook-node": "^1.1.8",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "js-yaml": "^4.1.0",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "mongoose": "^6.4.6",
        "morgan": "^1.10.0",
        "mysql2": "^2.3.3",
        "node-cron": "^3.0.2",
        "parseurl": "^1.3.3",
        "promise-mysql": "^5.1.0",
        "sequelize": "^6.21.4",
        "winston": "^3.6.0",
        "winston-daily-rotate-file": "^4.6.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.6",
        "@babel/core": "^7.17.8",
        "@babel/node": "^7.16.8",
        "@babel/plugin-proposal-class-properties": "^7.16.7",
        "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
        "@babel/plugin-transform-runtime": "^7.17.0",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-typescript": "^7.16.7",
        "@types/aws-sdk": "^2.7.0",
        "@types/chai": "^4.3.0",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/http-errors": "^1.8.2",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/lodash": "^4.14.180",
        "@types/mocha": "^9.1.0",
        "@types/morgan": "^1.9.3",
        "@types/node": "^17.0.23",
        "@types/node-cron": "^3.0.2",
        "@types/supertest": "^2.0.12",
        "@types/swagger-jsdoc": "^6.0.1",
        "@typescript-eslint/eslint-plugin": "^5.16.0",
        "@typescript-eslint/parser": "^5.16.0",
        "babel-plugin-module-resolver": "^4.1.0",
        "eslint": "^8.12.0",
        "sequelize-cli": "^6.4.1",
        "supertest": "^6.2.2",
        "swagger-jsdoc": "^6.1.0",
        "swagger-ui-express": "^4.3.0"
    }
}
