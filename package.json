{
  "name": "swagger-a-fume-server",
  "version": "0.0.1",
  "description": "향수 정보 서비스 A.fume Server Api 문서",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "_prestart": "npm install",
    "start": "cross-env NODE_ENV=development npm run build && node ./dist/index.js",
    "test": "cross-env NODE_ENV=test mocha --recursive --exit -t 5000 -r ts-node/register ./test/**/*.spec.ts ./test/**/*.spec.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check --watch",
    "build": "npm run build:js",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx,.js\" --source-maps inline"
  },
  "keywords": [
    "swagger"
  ],
  "license": "Unlicense",
  "private": true,
  "dependencies": {
    "aws-sdk": "^2.987.0",
    "chai": "^4.3.4",
    "connect": "^3.7.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.0.5",
    "mysql2": "^2.3.0",
    "node-cron": "^3.0.0",
    "promise-mysql": "^5.0.3",
    "sequelize": "^6.6.5",
    "swagger-tools": "^0.10.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/node": "^7.14.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-typescript": "^7.14.5",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.178",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "@types/chai": "^4.2.22",
    "@types/jsonwebtoken": "^8.5.5",
    "eslint": "^7.31.0",
    "supertest": "^6.1.6"
  }
}
